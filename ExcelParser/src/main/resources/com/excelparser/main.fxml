<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" prefHeight="355.0" prefWidth="495.0" spacing="20.0" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.excelparser.MainController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
    <children>
        <TextField fx:id="field_input" promptText="Введите путь к входному файлу" />
        <TextField fx:id="field_output" promptText="Введите путь к выходному файлу" />
        <Button fx:id="button" mnemonicParsing="false" text="Анализировать " />
    </children>
</VBox>